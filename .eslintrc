{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "globals": {
    "fabric": true,
    "document": true,
    "window": true,
    "Image": true
  },
  "rules": {
    "no-console": "off",
    "max-len": ["error", 128],
    "jsx-a11y/label-has-for": "off",
    "react/forbid-prop-types": "off",
  }
}
